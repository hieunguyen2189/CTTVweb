
@{
    ViewData["Title"] = "Factory Output Summary";
    ViewData["Show"] = "Inven";
    ViewData["Active"] = "OutSum";
}
<h1>@ViewData["Title"]</h1>
@section Stylesheets
{
<link href="~/assets/lib/flatpickr/flatpickr.min.css" rel="stylesheet">
}

<script>
$(function(){
    var lines = [];
    $.each($('#data').val().split(/\n/), function(i, line){
        if(line){
            lines.push(line);
        }
    });
    console.log(lines);
});
</script>
<body onload = "JavaScript:AutoRefresh(5000);">
<table id="SearchBySN" class="table table-sm table-dashboard  no-wrap mb-0 fs--1 w-100" >
    <thead class="bg-200 text-900">
        <tr>             
            <th>
               MODEL NAME
            </th>
            <th>
               DEEPC_OK
            </th>
            <th>
             DEEPC_NOK
            </th>
             <th>
               DEEPC_TOTAL
            </th>
            <th>
             CTTVOUT_OK
            </th>
             <th>
               CTTVOUT_NOK
            </th>
             <th>
              CTTVOUT_TOTAL
            </th>
            <th>
              CTTV_OK
            </th>
             <th>
              CTTV_NOK
            </th>
             <th>
             CTTV_TOTAL
            </th>
        </tr>
    </thead>
    <tbody class="list">
        @if (Model != null)
        {
            @foreach (var obj in Model)
            {
                <tr>
                    <td width="20%">
                        @obj.MODEL_NAME
                    </td>
                 
                     <td width="10%">
                        @obj.DEEPC_OK  
                    </td>
                     <td width="10%">
                        @obj.DEEPC_NOK  
                    </td>
                     <td width="10%">
                        @obj.DEEPC_TOTAL  
                    </td>
                     <td width="10%">
                        @obj.CTTVOUT_OK  
                    </td>
                    <td width="10%">
                        @obj.CTTVOUT_NOK
                    </td>
                     <td width="10%">
                        @obj.CTTVOUT_TOTAL  
                    </td>
                       <td width="10%">
                        @obj.CTTV_OK  
                    </td>
                    <td width="10%">
                        @obj.CTTV_NOK
                    </td>
                     <td width="10%">
                        @obj.CTTV_TOTAL  
                    </td>
                </tr>
            }
        }
    </tbody>
</table>

</body>

@section Scripts
{
<script src="~/js/custom.js"></script>
<script>

    var dataTable = $('#SearchBySN').DataTable({
        "sDom": "<'row mb-1' <'col-2'B><'col-2'l><'col-2' i><'col-2'f><'col-4'p>><'table-responsive't><'row'>",
        filter: true,
        "searching": true,
        "lengthMenu": [ [100, 200, 500, -1], [100, 200, 500, "All"] ],
        "scrollCollapse": true,
        "paging": true,
        // "bSort": true,
        "info": true,

        buttons: [

            {
                extend: 'excelHtml5',
                text: 'Excel',
                className: 'btn btn-primary hide',
                title: 'Data export',
                // titleAttr: 'Export all rows to Excel file',
            },

            
            {
                extend: 'copyHtml5',
                text: 'Copy Data',
                className: 'btn btn-primary hide',
                // titleAttr: 'Copy all rows to clipboard',
            },
        ],

    });


</script>
<script src="~/assets/lib/flatpickr/flatpickr.min.js"></script>
}