
@{
    ViewData["Title"] = "Factory Summary ";
    ViewData["Show"] = "Inven";
    ViewData["Active"] = "FacSum";
}
<h1>@ViewData["Title"]</h1>
@section Stylesheets
{
<link href="~/assets/lib/flatpickr/flatpickr.min.css" rel="stylesheet">
}

<script>
$(function(){
    var lines = [];
    $.each($('#data').val().split(/\n/), function(i, line){
        if(line){
            lines.push(line);
        }
    });
    console.log(lines);
});
</script>

<table id="SearchBySN" class="table table-sm table-dashboard  no-wrap mb-0 fs--1 w-100">
    <thead class="bg-200 text-900">
        <tr>
                        <th width="1%">
            </th>
            <th>
                MODEL NAME
            </th>
            <th>
               TOTAL PLAN  
            </th>
            <th>
                NOT PRODUCE
            </th>
             <th>
                IN FACTORY
            </th>
             <th>
                SHIPPING
            </th>

        </tr>
    </thead>
    <tbody class="list">
        @if (Model != null)
        {
           var counter = 1;
            @foreach (var obj in Model)
            {
                <tr>
                    <td width="1%">
                        @counter
                        @{
                            counter = counter + 1;
                        }
                    </td>
                    <td width="20%">
                        @obj.MODEL_NAME
                    </td>
                    <td width="20%">
                        @obj.TOTAL_PLAN  
                    </td>
                    <td width="20%">
                        @obj.NOT_PRODUCE
                    </td>
                     <td width="20%">
                        @obj.IN_FACTORY  
                    </td>
                    <td width="20%">
                        @obj.SHIPPING
                    </td>
                </tr>
            }
        }
    </tbody>
</table>



@section Scripts
{
<script src="~/js/custom.js"></script>

<script>

    var dataTable = $('#SearchBySN').DataTable({
        "sDom": "<'row mb-1' <'col-2'B><'col-2'l><'col-2' i><'col-2'f><'col-4'p>><'table-responsive't><'row'>",
        filter: true,
        "searching": true,
        "lengthMenu": [ [10, 100, 200, 500, -1], [10, 100, 200, 500, "All"] ],
        "scrollCollapse": true,
        "paging": true,
        // "bSort": true,
        "info": true,

        buttons: [

            {
                extend: 'excelHtml5',
                text: 'Excel',
                className: 'btn btn-primary hide',
                title: 'Data export',
                // titleAttr: 'Export all rows to Excel file',
            },

            
            {
                extend: 'copyHtml5',
                text: 'Copy Data',
                className: 'btn btn-primary hide',
                // titleAttr: 'Copy all rows to clipboard',
            },
        ],

    });


</script>
<script src="~/assets/lib/flatpickr/flatpickr.min.js"></script>
}